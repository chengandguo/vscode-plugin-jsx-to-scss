{
  "name": "jsx-to-scss",
  "displayName": "jsx-to-scss",
  "description": "Help you generate scss/less structure from jsx",
  "version": "0.1.2",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "webpack": "webpack --mode development",
    "test-compile": "tsc -p ./",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "publish": "npm run build && npx vsce package && npx vsce publish"
  },
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:jsx-to-scss.extract"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsx-to-scss.extract",
        "title": "JSX Extract Style to Clipboard"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == javascript || resourceLangId == javascriptreact",
          "command": "jsx-to-scss.extract"
        }
      ]
    },
    "keybindings": [
      {
        "command": "jsx-to-scss.extract",
        "key": "ctrl+e",
        "mac": "cmd+e"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "copy-paste": "^1.3.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vsce": "^2.15.0",
    "vscode-test": "^1.4.0",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "publisher": "joey",
  "repository": "https://github.com/chengandguo/vscode-plugin-jsx-to-scss.git"
}
